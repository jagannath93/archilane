<!DOCTYPE html>

<html>
<head>
{% load static %}
	
	<script type="text/JavaScript" src = "{{ STATIC_URL }}js/commonElements/jquery.min.js"></script>
	<script type="text/JavaScript" src = "{{ STATIC_URL }}js/commonElements/ajax_functions.js"></script>
	<script type="text/JavaScript">
		$(document).ready(function(){
			//alert("in ajax");
			$('.btn').bind('click', function(){
				$.ajaxSetup({ data: {csrfmiddlewaretoken: '{{ csrf_token }}' }, }); // Sending csrf_token with ajax post request
				get_content(this,"con","POST");
				get_app_navigation(this,"nav","POST");				
			});
		});
		
		$(document).ajaxStart(function(){
			msg = "Fetching data, please wait ...";
			$('#msg')[0].innerHTML = msg;
		}).ajaxStop(function(){
			msg = "";
			$("#msg")[0].innerHTML = msg;	
		   });

	</script>
</head>
